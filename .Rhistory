adduct <- function(F, mode = c('+', '-')){
pos_ion <- c('Li1', 'H3O1', 'N1H4', 'Na1', 'K1', 'C1H5O1', 'C2H4N1')
for (i in 1: length(pos_ion)) {
M_adduct <- vector(mode="character", length=length(pos_ion))
M_adduct[i] = paste(F, pos_ion[i], sep = '')
return(M_adduct)
}
}
